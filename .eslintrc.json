{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "vue/no-reserved-component-names": "off",
    "vue/multi-word-component-names": "off",
    "comma-dangle": ["error", "always-multiline"]
  }
}
